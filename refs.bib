@phdthesis{choi_2019,
	abstract = {A strongly interacting driven spin system can display a variety of interesting many- body quantum phenomena due to the interplay of interaction, disorder and coherent driving. Using a dense electronic spin ensemble of atom-like impurities in diamond, we study non-equilibrium quantum many-body dynamics involving âˆ¼10^6 spins. Specifically, we describe: (1) investigation of thermalization dynamics of a closed quantum system in the presence of long-range interactions and on-site disorder, (2) realization of new non-equilibrium phases of matter, such as discrete time crystals, and (3) probing emergent phase transitions in a driven many-body system. In addition, the high-density spin ensembles in the solid state also show great promise for practical sensing applications. As a practical application of these studies, we demonstrate high sensitivity magnetic-field sensing with fault-tolerant pulsed control. These studies demonstrate that dense electronic spin ensembles in the solid state can be a powerful tool to simulate quantum many-body dynamics, explore new coherent non-equilibrium phenomena and to detect external signals with superior sensitivity and high spectral and spatial resolution.},
	annote = {2020-05-13: Sekhar sent email with this attached},
	author = {Choi, Joonhee},
	date-added = {2020-08-05 17:05:34 -0600},
	date-modified = {2020-08-05 17:07:21 -0600},
