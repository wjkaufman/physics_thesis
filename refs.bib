	doi = {10.1002/cmr.a.21414},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/cmr.a.21414},
	journal = {Concepts in Magnetic Resonance Part A},
	keywords = {average Hamiltonian, average Hamiltonian theory, composite pulse, effective Hamiltonian, Magnus expansion},
	number = {6},
